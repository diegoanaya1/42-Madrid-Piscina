#include <stdio.h>

void	ft_ultimate_div_mod(int *a, int *b)

{
	int	div;
	int	mod;

	div = *a / *b;
	mod = *a % *b;
	*a = div;
	*b = mod;
}

int	main(void)
{
	int	a;
	int	b;

	a = 90;
	b = 5;
	ft_ultimate_div_mod(&a, &b);
	printf("%d" " - " "%d", a, b);
	return (0);
}
