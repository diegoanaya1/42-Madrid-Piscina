#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

void	ft_is_negative(int n)

{
	if (n < 0)
	{
		printf("N");
	}
	else
	{
		printf("P");
	}
}

int	main(void)

{
	ft_is_negative(0);
	return (0);
}
