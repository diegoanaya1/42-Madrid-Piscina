#include <stdio.h>

int	ft_str_is_printable(char *str)
{
	int	i;

	i = 0;
	while (str[i])
	{
		if (str[i] < 32 || str[i] > 126)
		{
			return (0);
		}
		i++;
	}
	return (1);
}

int	main(void)

{
	int	r;

	r = ft_str_is_printable("asdf");
	printf("Respuesta: %d\n", r);
	return (0);
}
